<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	
	<dubbo:application name="Idubbo-consumer"  />
	<dubbo:registry check="false" id="registry" address="zookeeper://172.16.22.104:2181"  />
	
	<!-- 公用服务 -->
	<dubbo:reference id="pubService" interface="com.ishidai.dubbo.api.PubService" registry="registry" timeout="30000"/>
	<dubbo:reference id="sendMQService" interface="com.ishidai.dubbo.api.SendMQService" registry="registry" />
	<dubbo:reference id="bankCardService" interface="com.ishidai.dubbo.api.BankCardService" registry="registry" />
	<dubbo:reference id="smsService" interface="com.ishidai.dubbo.api.SmsService" registry="registry" />
	<dubbo:reference id="ruleEngineService" interface="com.ishidai.dubbo.api.RuleEngineService" registry="registry" />
	<!-- 汽车之家服务 -->
	<dubbo:reference id="loanService" interface="com.ishidai.dubbo.api.car.LoanService" registry="registry"/>
	<!-- 51信用卡服务 -->
	<dubbo:reference id="loan51Service" interface="com.ishidai.dubbo.api.card51.Loan51Service" registry="registry"/>
	<!-- 身份认证服务 -->
	<dubbo:reference id="IdentityService" interface="com.ishidai.dubbo.api.IdentityService" registry="registry" />
	
</beans>